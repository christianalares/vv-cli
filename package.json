{
  "name": "vinnovera-cli",
  "version": "0.0.3",
  "description": "A CLI tool for the people that works at Vinnovera",
  "main": "./src/index.js",
  "bin": {
    "vv": "./src/index.js"
  },
  "preferGlobal": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "scripts": {
    "lint": "eslint '**/*.js' --ignore-path .gitignore",
    "dev": "nodemon index.js",
    "build": "webpack",
    "start": "node index.js"
  },
  "keywords": [],
  "author": "Christian Alares",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.1",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "dotenv": "^8.0.0",
    "inquirer": "^6.3.1",
    "ora": "^3.4.0",
    "os": "^0.1.1",
    "ramda": "^0.26.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^1.19.1",
    "prettier": "^1.19.1"
  }
}
